# Required for Mapbox GL map rendering. Get from your Mapbox account.
VITE_MAPBOX_ACCESS_TOKEN=

# Optional auth key (frontend-only publishable key). Get from Clerk dashboard.
VITE_CLERK_PUBLISHABLE_KEY=

# Optional weather raster API base for local/prod switching.
# Default: https://api.offshoreweatherrouting.com
VITE_WEATHER_API_BASE=

# Optional weather model source URL read by the weather API.
# Default: https://data.dynamical.org/noaa/gfs/forecast/latest.zarr
VITE_WEATHER_SOURCE_URL=

# Optional routing SSE endpoint for local/prod switching.
# Default: https://peterm790--weather-routing-get-route.modal.run
VITE_ROUTING_STREAM_URL=
